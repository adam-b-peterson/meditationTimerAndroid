<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.replicatecountdowntimer">

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_lotus"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_lotus"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".BaseActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SquaredBreathing.SquaredBreathingTimerActivity"
            android:label="@string/title_activity_squared_breathing_timer"></activity>
        <activity
            android:name=".SquaredBreathing.SquaredBreathingSettingActivity"
            android:label="@string/title_activity_sqaured_breathing_setting"></activity>
        <activity android:name=".Setting.SettingActivity" />
        <activity android:name=".Scoreboard.ScoreboardActivity" />
        <activity android:name=".InstructionActivity"></activity>
    </application>

</manifest>